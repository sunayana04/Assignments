    "class Point:\n",
    "\n",
    "    def __init__(self,x,y,z):\n",
    "        self.x = x\n",
    "        self.y = y\n",
    "        self.z = z\n",
    "\n",
    "    def sqSum(self):\n",
    "        a=self.x**2\n",
    "        b=self.y**2\n",
    "        c=self.z**2\n",
    "        print(\"The sum of the squares= \",a+b+c)\n",
    "p1=Point(1,3,5)\n",
    "p1.sqSum()"
   ]
